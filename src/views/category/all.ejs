<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Categories</title>
</head>
<body>
    <%- include('../nav') %>
    <main>
        <h2>Categories Page</h2>
        <a href="/category/new">New Category</a>
        <% for (const row of rows) { %>
            <div>
                <h3><a href=<%- `/category/${row.cat_id}` %>><%= row.cat_name %></a></h3>
                <p><%= row.cat_desc %></p>
                <p><%= row.cat_id %></p>
            </div>
        <% } %>
    </main>
</body>
</html>